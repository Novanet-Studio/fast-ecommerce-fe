{
  "search.exclude": {
    "**/node_modules": true,
    "**/*.code-search": true,
    "yarn.lock": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "css.lint.unknownAtRules": "ignore",
  "editor.quickSuggestions": {
    "strings": true
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  }
}
